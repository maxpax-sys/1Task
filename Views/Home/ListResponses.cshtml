@model IEnumerable<Task1_1.Models.GuestResponse>
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <title>ListResponses</title>
</head>
<body>
    <div class="panel-body">
        <h2>Список кто идет</h2>
    </div>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Имя</th>
                <th>Email</th>
                <th>Номер телефона</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Task1_1.Models.GuestResponse m in Model)
            {
                <tr>
                    <td>@m.Name</td>
                    <td>@m.Email</td>
                    <td>@m.Phone</td>
                </tr>


            }
        </tbody>

    </table>
</body>
</html>